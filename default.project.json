{
  "name": "zulu-site",
  "globIgnorePaths": [
    "**/package.json",
    "**/tsconfig.json"
  ],
  "tree": {
    "$className": "DataModel",
    "ServerScriptService": {
      "$className": "ServerScriptService",
      "TS": {
        "$path": "out/server"
      }
    },
    "ReplicatedStorage": {
      "$className": "ReplicatedStorage",
      "rbxts_include": {
        "$path": "include",
        "node_modules": {
          "$className": "Folder",
          "@rbxts": {
            "$path": "node_modules/@rbxts"
          },
          "@flamework": {
            "$path": "node_modules/@flamework"
          }
        }
      },
      "Shared": {
        "$path": "out/shared"
      },
      "Client": {
        "$path": "out/client"
      },
      "TS": {
        "$path": "out/shared"
      }
    },
    "StarterPlayer": {
      "$className": "StarterPlayer",
      "$properties": {
        "CameraMinZoomDistance": 10,
        "EnableMouseLockOption": false,
        "AutoJumpEnabled": true
      },
      "StarterPlayerScripts": {
        "$className": "StarterPlayerScripts",
        "TS": {
          "$path": "out/client"
        }
      }
    },
    "Workspace": {
      "$className": "Workspace",
      "$properties": {
        "FilteringEnabled": true
      }
    },
    "HttpService": {
      "$className": "HttpService",
      "$properties": {
        "HttpEnabled": true
      }
    },
    "SoundService": {
      "$className": "SoundService",
      "$properties": {
        "RespectFilteringEnabled": true
      }
    }
  }
}